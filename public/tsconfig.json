{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es5",
    "jsx": "react",
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "lib": ["es6", "dom"],
    "baseUrl": ".",
    "paths": {
      "*": ["../node_modules/*", "./types/*"]
    },
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
  }
}
